#import "Music.h"


@implementation Music
@synthesize title, album, artist, description, bundleName, credit;
- (id)initWithName:(NSString *)t 
            artist:(NSString *)art
		bundleName:(NSString *)bundle
	         album:(NSString *)alb 
       description:(NSString *)desc
            credit:(NSString *)cred {
	
	self.title = t;
    self.artist = art;
    self.bundleName = bundle;
    self.album = alb;
    self.description = desc;
    self.credit = cred;
	return self;
}

@end
